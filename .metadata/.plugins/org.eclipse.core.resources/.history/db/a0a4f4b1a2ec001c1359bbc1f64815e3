package Genrics;

//Single parameter
public class AllPrinter <T>{

	private T whatToPrint;

	public AllPrinter(T whatToPrint) {
		this.whatToPrint = whatToPrint;
	} 
	
	void printing()
	{
		System.out.println(whatToPrint);
	}
	
}

public class AllPrintermultiple <T,T1>{

	private T whatToPrint1;
	private T1 whatToPrint2;
	
	
	void printing()
	{
		System.out.println(whatToPrint);
	}
	
}
